    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.9.0",
    "vite": "^4.4.0",
    "typescript": "^5.4.4",
    "tailwindcss": "^3.6.7",
    "postcss": "^8.4.26",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.55.0",
    "@typescript-eslint/parser": "^5.67.0",
    "@typescript-eslint/eslint-plugin": "^5.67.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.9.1",
    "eslint-plugin-react": "^7.31.6",
    "vitest": "^0.36.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.6",
    "@testing-library/user-event": "^14.4.3",
