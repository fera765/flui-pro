{
  "name": "frontend-web-react-ts-tailwind",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json,md}\"",
